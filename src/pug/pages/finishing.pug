extends ../layouts/base

append vars
	- title = 'Отделка';
	- body['classList'] = 'is-loaded';

block content
	+b.SECTION.fullscreen
		+e.container._fix._header
			include ../layouts/header
			include ../layouts/nav
		+b.page
			+e.head
				+e.head-wrap.wrap.flex._justify
					+e.H1.title.h1
						| Отделка
					+e.UL.breadcrumbs.breadcrumbs__
						+e.LI.item
							+e.A.link(href="main.html") Главная
						+e.LI.item
							+e.SPAN.current Отделка
			+e.finishing.finishing__
				+e.wrap.wrap
					+e.box
						+e.gallery.owl-carousel.js-gallery
							+e.A.item(href="/app/img/finishing__img1.jpg")
								img(src="/img/finishing__img1.jpg")
							+e.A.item(href="/app/img/finishing__img2.jpg")
								img(src="/img/finishing__img2.jpg")
							+e.A.item(href="/app/img/finishing__img3.jpg")
								img(src="/img/finishing__img3.jpg")
							+e.A.item(href="/app/img/finishing__img1.jpg")
								img(src="/img/finishing__img1.jpg")
							+e.A.item(href="/app/img/finishing__img2.jpg")
								img(src="/img/finishing__img2.jpg")
							+e.A.item(href="/app/img/finishing__img3.jpg")
								img(src="/img/finishing__img3.jpg")
						+e.H2.title
							| Белая отделка
						+e.row.flex_justify
							+e.column

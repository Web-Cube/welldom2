+b.SECTION.contact
	+e.wrap.wrap
		+e.H2.title.h1.h1_small Наше местоположение
		+e.list.flex._justify
			+e.A.item._address.js-address.is-active(href="#map-footer1")
				+e.label Адрес офиса:
				+e.text
					+e.SPAN.text-icon
						+icon('location-pointer','0 0 50 80')
					| Санкт-Петербург, 
					br
					| ул. Марата, д.75
				+e.link
					+e.link-text Показать на карте
					+e.link-icon
						+icon('location-pin','0 0 491.582 491.582')
			+e.A.item._address.js-address(href="#map-footer2")
				+e.label Адрес офиса:
				+e.text
					+e.SPAN.text-icon
						+icon('location-pointer','0 0 50 80')
					| Санкт-Петербург, 
					br
					| ул. Заставская 904
				+e.link
					+e.link-text Показать на карте
					+e.link-icon
						+icon('location-pin','0 0 491.582 491.582')
			+e.item
				+e.label Телефон:
				+e.text
					+e.SPAN.text-icon._phone
						+icon('phone','0 0 15 21')
					a(href="tel:+78126465130") +7 (812) 646-51-30
			+e.item
				+e.label Почта:
				+e.text
					+e.SPAN.text-icon._email
						+icon('email','0 0 14 14')
					a(href="mailto:namename@yandex.ru") namename@yandex.ru
	+e.map-list
		+e.A.button.flat-button__.js-modal(href="#make")
			+e.text Записаться на просмотр
		+e.map-wrap
			+e.map.map-box#map-footer1(data-len="59.920760", data-lng="30.341783")
			+e.map.map-box#map-footer2(data-len="59.888839", data-lng="30.328642")